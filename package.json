{
    "name": "minehunt",
    "version": "0.0.1",
    "private": true,
    "license": "MIT",
    "description": "Minesweeper built with react-native. Play Anywhere, Anytime.",
    "author": "Sobhan Bera",
    "homepage": "https://github.com/sobhanbera/minehunt",
    "repository": {
        "type": "git",
        "url": "https://github.com/sobhanbera/minehunt.git"
    },
    "scripts": {
        "test": "jest",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "reset": "yarn start --reset-cache",
        "prettier-check": "yarn prettier -c app",
        "prettier-format": "yarn prettier --write app && git status",
        "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "build": "cd android && ./gradlew bundleRelease",
        "bd": "cd android && ./gradlew bundleRelease",
        "prepare": "husky install",
        "tree": "tree app",
        "lines": "find . -regex \".*\\.\\(js\\|jsx\\|ts\\|tsx\\|json\\)\" ! -path \"./node_modules/*\" | xargs wc -l",
        "tslines": "find . -name '*.js*' ! -path \"./node_modules/*\" ! -path \"./android/*\" ! -path \"./ios/*\" | xargs wc -l",
        "jslines": "find . -name '*.ts*' ! -path \"./node_modules/*\" ! -path \"./android/*\" ! -path \"./ios/*\" | xargs wc -l",
        "tjslines": "find . -regex \".*\\.\\(js\\|jsx\\|ts\\|tsx\\|json\\)\" ! -path \"./node_modules/*\" ! -path \"./android/*\" ! -path \"./ios/*\" | xargs wc -l"
    },
    "dependencies": {
        "@react-native-community/async-storage": "^1.12.1",
        "@react-native-community/blur": "^3.6.0",
        "@react-native-community/slider": "^4.1.12",
        "@react-navigation/native": "^6.0.6",
        "@react-navigation/stack": "^6.0.11",
        "axios": "^0.24.0",
        "dayjs": "^1.10.7",
        "i18next": "^21.6.0",
        "react": "17.0.2",
        "react-i18next": "^11.15.0",
        "react-native": "0.66.3",
        "react-native-animatable": "^1.3.3",
        "react-native-device-info": "^8.4.8",
        "react-native-linear-gradient": "^2.5.6",
        "react-native-locale-detector": "^1.0.1",
        "react-native-vector-icons": "^9.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/runtime": "^7.12.5",
        "@react-native-community/eslint-config": "^2.0.0",
        "@types/jest": "^26.0.23",
        "@types/react-native": "^0.66.4",
        "@types/react-native-vector-icons": "^6.4.10",
        "@types/react-test-renderer": "^17.0.1",
        "babel-jest": "^26.6.3",
        "eslint": "^7.14.0",
        "husky": "^7.0.4",
        "jest": "^26.6.3",
        "metro-react-native-babel-preset": "^0.66.2",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.2",
        "react-test-renderer": "17.0.2",
        "typescript": "^4.4.4",
        "typescript-language-server": "^0.8.1"
    },
    "resolutions": {
        "@types/react": "^17"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "keywords": [
        "minesweeper",
        "mine hunt",
        "minehunt",
        "offline apps",
        "react-native",
        "react",
        "offline games",
        "react-native game",
        "find the mine",
        "play game",
        "game"
    ]
}
